<div class="content-title top-layout"> 
    <h2>Our chef recommendation</h2>
</div>
<div>
    <div class="align-center">
        {{#each Menu}}
        <div class="img-box">
            <img src="/img/{{this.imgPath}}" alt="pic">
            <span>
                {{this.description}}
                {{this.price}}
            </span>

        </div>
        {{/each}}
    </div>
</div>